{
  "manifest_version": 3,
  "name": "Zendesk SOC Hunter",
  "version": "1.0",
  "description": "Threat Intelligence tool for Zendesk and SIEM platforms.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "fedesella95@gmail.com",
      "strict_min_version": "109.0",
      "data_collection_permissions": {
        "required": [
          "none"
        ]
      }
    }
  }
}